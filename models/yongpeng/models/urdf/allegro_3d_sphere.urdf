<robot name="sphere_r0.06m" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- SPHERE (MANIPULAND) -->
  <link name="sphere_link">
    <visual>
      <geometry>
        <sphere radius="0.06"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0 "/>
      <material name="milkwhite">
        <color rgba="1.0 0.9843 0.9412 0.8" />
      </material>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.06" />
        <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0" />
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0" />
      <mass value="0.1"/>
      <inertia ixx="1e-4" ixy="0.0" ixz="0.0" iyy="1e-4" iyz="0.0" izz="1e-4"/>
    </inertial>
  </link>

</robot>
